@model IEnumerable<ArabaDTO>
@{ Layout = "_MasterLayout"; ViewBag.Title = "Test";}

<h2>All Cars</h2>

<table class="table table-sm table-striped table-bordered m-2">
    <thead>
        <tr>
            <th>Marka</th>
            <th>Model</th>
            <th>Şanzıman</th>
            <th>Vites</th>
            <th>Yakıt</th>
            <th>Fiyat</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var r in Model)
        {
            <tr>
                <td>@r.Marka</td>
                <td>@r.Model</td>
                <td>@r.Sanziman</td>
                <td>@r.Vites</td>
                <td>@r.Yakit</td>
                <td>@r.Fiyat</td>
            </tr>
        }
    </tbody>
</table>